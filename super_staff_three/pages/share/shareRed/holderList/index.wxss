.content{
  width: 100%;
  border-top: 2rpx solid #eee;
}
.section{
  width: 93%;
  height: 80rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 28rpx;
  color: #353535;
  margin: 0 auto;
}
.none_none{
  width: 100%;
  margin: 0 auto;
}
.shareNone{
width:230rpx;
display:flex;
flex-direction:column;
align-items:center;
margin:-140rpx 0 0 -114rpx;
height:280rpx;
position:fixed;
top:50%;
left: 50%;
}
.shareNone image{
width: 230rpx;
height:200rpx;
margin: 0 auto;
}
.shareNone .none_txt{
  font-size: 32rpx;
  color: #888888;
  margin-top: 30rpx;
}