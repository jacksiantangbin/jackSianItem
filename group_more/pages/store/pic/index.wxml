<view class='box'>
  <view class='content'>
    <block wx:for="{{storeImageList}}" wx:key="">
      <view class='item'>
        <image class="image"  src='{{item.imageUrl}}' mode="widthFix" data-Id='{{item.id}}' catchtap='previewImage2' data-url="{{item}}" data-imagelist="{{storeImageList}}"></image>
      </view>
    </block>
  </view>
</view>