/* 股东弹窗 */

.modal-mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.3;
  overflow: hidden;
  z-index: 9000;
  color: #fff;
}

.modal-dialog {
  width: 520rpx;
  height: 620rpx;
  overflow: hidden;
  position: fixed;
  top: 50%;
  left: 50%;
  z-index: 9999;
  /* background:#f9f9f9; */
  margin: -350rpx 0 0 -260rpx;
  border-radius: 36rpx;
  padding-bottom: 100rpx;
}

.modal-dialog .redCover {
  width: 520rpx;
  height: 620rpx;
  margin-bottom: 24rpx;
}

.redTxt {
  width: 80%;
  font-size: 36rpx;
  color: #353535;
  position: absolute;
  top: 40rpx;
  left: 50rpx;
  display: flex;
  justify-content: center;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  text-align: center;
}

.redClose {
  width: 50rpx;
  height: 50rpx;
  display: flex;
  margin: 0 auto;
}

.swiper-block {
  width: 100%;
  /* margin-top: 10rpx; *//* margin-bottom: 10rpx; */
}

.top_swp {
  width: 100%;
  height: 310rpx;
  display: flex;
  align-items: center;
}

.swiper-item {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: flex-start;
  overflow: unset;
}

.slide-image {
  height: 230rpx;
  width: 530rpx;
  border-radius: 10rpx;
  box-shadow: 0px 0px 10rpx rgba(0, 0, 0, 0.2);
  /* margin: 0rpx 20rpx; */
  z-index: 1;
  /* position: relative; */
  background: -webkit-linear-gradient(left, #8ed9ec, #5bc1e2);
}

.slide-image2 {
  height: 230rpx;
  width: 100%;
  border-radius: 10rpx;
  box-shadow: 0px 0px 10rpx rgba(0, 0, 0, 0.2);
  margin: 0 auto;
  z-index: 1;
  /* position: relative; */
  background: -webkit-linear-gradient(left, #8ed9ec, #5bc1e2);
}

.active {
  transform: scale(1.14);
  transition: all 0.2s ease-in 0s;
  z-index: 20;
}

.ac_st_center {
  width: 490rpx;
  height: 190rpx;
  padding: 20rpx;
  display: flex;
  /* position: absolute; */
}

.ac_st_center2 {
