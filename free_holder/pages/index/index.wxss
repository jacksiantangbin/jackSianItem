  margin-left: 30rpx;
  height: 190rpx;
  display: flex;
  align-items: center;
}

.ac_st_tit {
  width: 100%;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
}

.storeType {
  width: 223rpx;
