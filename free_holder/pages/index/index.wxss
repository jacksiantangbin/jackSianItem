  margin: 0 30rpx;
}

.swiper-block .swiper-item:nth-child(2n) .slide-image {
  background: -webkit-linear-gradient(left, #fad0c4, #ff9a9e);
}

.swiper-block .swiper-item:nth-child(3n) .slide-image {
  background: -webkit-linear-gradient(left, #fda588, #ea7167);
}

.swiper-block .swiper-item:nth-child(4n) .slide-image {
  background: -webkit-linear-gradient(left, #ffafbd, #ffc3a0);
}

.swiper-block .swiper-item:nth-child(5n) .slide-image {
  background: -webkit-linear-gradient(left, #d3b987, #ecd3a1);
}

.swiper-block .swiper-item:nth-child(6n) .slide-image {
  background: -webkit-linear-gradient(left, #6bb7ec, #72d3c1);
}

